
# Python program to find roots of quadratic equation

import math

# function for finding roots
def findroots(a,b,c):
  dis_form = b*b - 4*a*c
  sqrt_val = math.sqrt(abs(dis_form))

  if dis_form > 0:
    print("real and different roots")
    print((-b - sqrt_val) / (2*a))
    print((-b + sqrt_val) / (2*a))

  elif dis_form == 0:
    print("real and same roots")
    print(-b/(2*a))
