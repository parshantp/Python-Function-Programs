# defining a function

def lcm(x,y):
  selecting the greater number
  if x>y:
    greater = x
  else:
    greater = y

  while(True):
    if((greater % x == 0) and (greater % y == 0)):
      lcm = greater
      break
    greater += 1
  return lcm


# taking input from user
x = int(input("Enter first number"))
y = int(input("enter second number"))

print(f"The LCM of {x} and {y} is {lcm(x,y)}")
